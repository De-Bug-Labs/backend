{
   "name": "backend",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "start": "ts-node src/index.ts",
      "test": "jest dist/tests",
      "test-src": "jest dist/tests --testPathIgnorePatterns=\"dist/tests/orm\"",
      "test-orm": "jest dist/tests/orm",
      "build": "npm run format && npm run eslint && tsc",
      "serve": "nodemon dist/src",
      "serve-debug": "nodemon --inspect=127.0.0.1:9230 dist/src",
      "watch": "concurrently -k -p \"[{name}]\" -n \"Typescript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
      "watch-ts": "tsc -w",
      "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Typescript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
      "format": "npx prettier --write \"{src,tests}/**/*.ts\"",
      "eslint": "eslint --cache --color . --ext .ts",
      "eslint-fix": "eslint --cache --color --fix . --ext .ts",
      "eslint-orm-fix": "eslint --cache --color --fix src/orm/. --ext .ts",
      "typeorm": "ts-node ./node_modules/typeorm/cli.js",
      "migration-create": "npm run typeorm migration:generate -- -n \"$npm_config_name\" && npm run eslint-orm-fix",
      "migration-run": "npm run typeorm migration:run",
      "migration-revert": "npm run typeorm migration:revert"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/De-Bug-Labs/backend.git"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/De-Bug-Labs/backend/issues"
   },
   "homepage": "https://github.com/De-Bug-Labs/backend#readme",
   "devDependencies": {
      "@types/express": "^4.17.11",
      "@types/jest": "^26.0.22",
      "@types/node": "^14.14.37",
      "@typescript-eslint/eslint-plugin": "^4.20.0",
      "@typescript-eslint/parser": "^4.20.0",
      "concurrently": "^6.0.1",
      "jest": "^26.6.3",
      "nodemon": "^2.0.7",
      "prettier": "^2.2.1",
      "swagger-jsdoc": "^7.0.0-rc.6",
      "ts-node": "^9.1.1",
      "typescript": "^4.2.3"
   },
   "dependencies": {
      "consolidate": "^0.16.0",
      "dotenv": "^8.2.0",
      "eslint": "^7.23.0",
      "eslint-config-prettier": "^8.1.0",
      "eslint-plugin-prettier": "^3.3.1",
      "express": "^4.17.1",
      "moment": "^2.29.1",
      "npx": "^10.2.2",
      "pg": "^8.4.0",
      "swagger-express-mw": "^0.7.0",
      "swagger-node-runner": "^0.7.3",
      "swagger-ui-express": "^4.1.6",
      "typeorm": "^0.2.32",
      "yamljs": "^0.3.0"
   }
}
